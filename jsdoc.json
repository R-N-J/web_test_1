{
  "source": {
    "include": ["js"],
    "includePattern": ".+\\.ts$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleanslate": {
      "staticFiles": {
        "include": ["img"]
      }
    }
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "README.md"
  }
}
